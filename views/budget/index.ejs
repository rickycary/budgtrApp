<%- include("../partials/head.ejs") %>
<body>

    <div class="container">

        <h1>Budgtr</h1>

        <a href="/budget/new">Create New Budget</a>

        <table class="u-full-width">
            <% budgets.forEach((budget, index) => { %>


            <tbody>
                <td><%= budget.date %></td>
                <td><a href="/budget/<%= index %>"><%= budget.name %></a></td>
                <td><%= budget.amount %></td>
                  
        </tbody>
        <%  }) %>

        </table>

    </div>
    
</body>
</html>